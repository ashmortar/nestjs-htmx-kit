{
  "name": "nest-jsx",
  "version": "0.0.1",
  "description": "",
  "author": "Aaron Ross",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "copy:flowbite": "cp -r node_modules/flowbite/dist/flowbite.min.js public/flowbite.min.js",
    "build:css": "npm run copy:flowbite && tailwindcss -i ./src/tailwind.css -o ./public/styles.css --minify",
    "watch:css": "npm run copy:flowbite && tailwindcss -i ./src/tailwind.css -o ./public/styles.css --watch",
    "build": "npm run prisma:generate && npm run build:css && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run build:css && nest start",
    "start:dev": "concurrently \"npm run watch:css\" \"nest start --watch\"",
    "start:debug": "npm run build:css && nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "xss-scan && NODE_ENV=test jest",
    "test:watch": "xss-scan && NODE_ENV=test jest --watch",
    "test:cov": "xss-scan && NODE_ENV=test jest --coverage",
    "test:debug": "xss-scan && NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "migrate:dev": "prisma migrate dev",
    "migrate:dev:create": "prisma migrate dev --create-only",
    "migrate:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:seed": "prisma db seed"
  },
  "dependencies": {
    "@anatine/zod-mock": "3.13.4",
    "@anatine/zod-nestjs": "2.0.9",
    "@anatine/zod-openapi": "2.2.6",
    "@kitajs/html": "4.1.0",
    "@kitajs/ts-html-plugin": "4.0.1",
    "@nestjs/common": "10.3.10",
    "@nestjs/config": "3.2.3",
    "@nestjs/core": "10.3.10",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/mapped-types": "2.0.5",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.3.10",
    "@nestjs/swagger": "7.4.0",
    "@opentelemetry/auto-instrumentations-node": "0.49.1",
    "@opentelemetry/exporter-jaeger": "1.25.1",
    "@opentelemetry/exporter-prometheus": "0.52.1",
    "@opentelemetry/instrumentation-pino": "0.41.0",
    "@opentelemetry/sdk-node": "0.52.1",
    "@prisma/client": "5.17.0",
    "@quixo3/prisma-session-store": "3.1.13",
    "bcrypt": "5.1.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cookie-parser": "1.4.6",
    "date-fns": "3.6.0",
    "flowbite": "2.5.1",
    "helmet": "7.1.0",
    "jest-mock-extended": "3.0.7",
    "lodash": "4.17.21",
    "nestjs-i18n": "10.4.5",
    "nestjs-otel": "6.1.1",
    "nestjs-pino": "4.1.0",
    "nestjs-prisma": "0.23.0",
    "openapi3-ts": "4.3.3",
    "passport": "0.7.0",
    "passport-google-oauth2": "0.2.0",
    "passport-google-oidc": "0.1.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "pino-http": "10.2.0",
    "pino-pretty": "11.2.2",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "7.24.7",
    "@faker-js/faker": "8.4.1",
    "@nestjs/cli": "10.4.2",
    "@nestjs/schematics": "10.1.3",
    "@nestjs/testing": "10.3.10",
    "@swc/cli": "0.4.0",
    "@swc/core": "1.7.6",
    "@types/bcrypt": "5.0.2",
    "@types/cookie-parser": "1.4.7",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/lodash": "4.17.7",
    "@types/node": "20.14.15",
    "@types/passport-google-oauth2": "0.1.8",
    "@types/passport-jwt": "4.0.1",
    "@types/passport-local": "1.0.38",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.16.1",
    "@typescript-eslint/parser": "7.16.1",
    "concurrently": "8.2.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "flowbite-typography": "1.0.3",
    "jest": "29.7.0",
    "picocolors": "1.0.1",
    "prettier": "3.3.3",
    "prisma": "5.17.0",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "tailwindcss": "3.4.9",
    "ts-jest": "29.2.4",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tsx": "4.17.0",
    "typescript": "5.5.3",
    "zod-prisma-types": "3.1.8"
  },
  "browserslist": "last 2 versions",
  "jest": {
    "reporters": [
      [
        "github-actions",
        {
          "silent": false
        }
      ],
      "summary"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "jsx",
      "tsx"
    ],
    "coveragePathIgnorePatterns": [
      "main.ts"
    ],
    "rootDir": ".",
    "testRegex": [
      "src/.*\\.spec\\.ts$",
      "test/.*\\.e2e-spec\\.ts$"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest",
      "\\.[jt]sx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s",
      "src/**/*.(t|j)sx"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@core/(.*)": "<rootDir>/src/$1",
      "@generated/(.*)": "<rootDir>/generated/$1"
    }
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
